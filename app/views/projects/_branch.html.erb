<div id="branch-main-content" class="container">
  <div class="row">
    <h1 class="page-title"><%= page_title %></h1>
    <%= render 'projects/branch/create_new_project', branch: branch %>
  </div><!-- row -->

  <div class="row">
    <%= render  'projects/branch/search_form', 
                branch: branch,
                search_placeholder: search_placeholder %>
  </div><!-- row -->

  <div class="row">
    <%= render 'projects/branch/categories', branch: branch %>
  </div>

  <div class="row">
    <div class="col-sm-12" id="feed">
      <%= render @projects %>
      <%= render no_projects_partial_path(@projects) %>			
    </div>
  </div><!-- row -->	
  
  <div class="infinite-scroll">
    <%= will_paginate @projects %>
  </div>
</div>